	ns7:membership	ns29:i_dsk_ps_bab ;
	skos:altLabel	"Basic or above basic Digital Skills - Problem solving domain" ;
	skos:prefLabel	"Basic or above basic Digital Skills - Problem solving domain" .
ns1:i_dsk_s_bab	rdf:type	ns3:Indicator ,
		skos:Concept ;
	skos:notation	"i_DSK_S_BAB" ;
