ns1:e_gov_ebus	rdf:type	ns2:Indicator ,
		skos:Concept ;
	skos:prefLabel	"Digital Public Services for Businesses (up to 2020)" ;
	dct:source	ns5:egov_bench ;
	skos:notation	"e_gov_ebus" ;
	skos:altLabel	"Digital Public Services for Businesses (up to 2020)" ;
